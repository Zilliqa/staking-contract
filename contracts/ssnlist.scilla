(* @dev:  Drain the contract's balance. Used by current admin only *)
(*        in event of contract upgrade or emergency. *)
(* @param initiator: The original caller who called the proxy *)
(* @param amt: The amount (Qa) to be drained from the contract back to the admin address *)
transition DrainContractBalance(amt: Uint128, initiator: ByStr20)
  IsPaused;
  IsProxy;
  IsAdmin initiator;
  bal <- _balance;
  less_than = builtin lt bal amt;
  match less_than with
  | True => throw
  | False =>
    TransferFunds addfunds_tag amt initiator
  end
end

(***************************************************)
(*           Contract state migration              *)
(***************************************************)

(* Common validation procedure for all migration transitions *)
procedure ValidateMigration (initiator : ByStr20)
  IsPaused;
  IsProxy;
  IsAdmin initiator
end

(***************************************************)

procedure MigrateCommForSsnByKey (
  arg : Pair ByStr20 (ByStr20 with contract
                                     field comm_for_ssn: Map ByStr20 (Map Uint32 Uint128)
                              end))
  match arg with
  | Pair key old_contract =>                          
    old_val_opt <-& old_contract.comm_for_ssn[key];
    match old_val_opt with
    | Some old_val =>
      comm_for_ssn[key] := old_val
    | None => (* Nothing to migrate *)
    end
  end
end

transition MigrateCommForSsn (
  initiator : ByStr20,
  keys : List ByStr20,
  old_contract : ByStr20 with contract
                                field comm_for_ssn: Map ByStr20 (Map Uint32 Uint128)
                         end)
  ValidateMigration initiator;
  mapper = @list_map_to_pair_with_constant ByStr20 (ByStr20 with contract
