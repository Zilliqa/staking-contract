let decide_n = 
  fun(m : Uint32) =>
  fun(n : Uint32) =>
  fun(upper: Uint32) =>
  let k = builtin sub n m in
  let more_than_upper = uint32_gt k upper in
  match more_than_upper with
  | True =>
    builtin add m upper
  | False => n
  end

